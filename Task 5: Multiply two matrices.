print("Enter m (rows of A): ")
m = parse(Int, readline())

print("Enter n (columns of B): ")
n = parse(Int, readline())

print("Enter k (columns of A / rows of B): ")
k = parse(Int, readline())

A = rand(1:10, m, k)
B = rand(1:10, k, n)
C = A * B

println("Matrix A:")
println(A)

println("Matrix B:")
println(B)

println("Matrix C = A * B:")
println(C)
